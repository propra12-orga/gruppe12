
public class Spielfeld implements Objekte {
	public int Breite,Hoehe;
	
	
	public Spielfeld (int Breite,int Hoehe){ // Konstruker der Breite & Hoehe entgegennimmt
		Hoehe=this.Hoehe;
		Breite=this.Breite;
		int Register[][]=new int [Breite][Hoehe]; // Initialisierung des Registers
	for(int x=0;x<Breite;x++){
		for(int y=0;y<Hoehe;y++){
			Register[x][y]=1;
		}
	}
	
	}

	
	public int getX() {
		
		return 0;
	}

	
	public int getY() {
		
		return 0;
	}

	
	public int setX() {
		
		return 0;
	}

	
	public int setY() {
		
		return 0;
	}
}
