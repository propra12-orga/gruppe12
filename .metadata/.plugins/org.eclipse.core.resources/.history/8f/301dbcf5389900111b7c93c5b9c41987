public class Spielfeld implements Objekte {
	int Hoehe, Breite;
	int Register[][];

	public Spielfeld(int Breite, int Hoehe) {
		Register = new int[Breite][Hoehe];
	}

	public void fill(int RegX, int RegY, int Obj) {
		Register[RegX][RegY] = Obj;
	}

	public int getObj(int RegX,int RegY){
		
		return Register[Regx][RegY];
	}
	
	
	
	
	
	@Override
	public int getX(int x, int y) {

		return Register[x][y];
	}

	@Override
	public int getY(int x, int y) {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public int setX() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public int setY() {
		// TODO Auto-generated method stub
		return 0;
	}

}